<jmx_template>
	<configuration></configuration>
	<graphs>
		<graph>
			<name>CPU Monitoring</name>
			<means>
				<mbean>
					<canonical_name>java.lang:type=OperatingSystem</canonical_name>
					<attribute_name>ProcessCpuLoad</attribute_name>
				</mbean>
				<mbean>
					<canonical_name>java.lang:type=OperatingSystem</canonical_name>
					<attribute_name>SystemCpuLoad</attribute_name>
				</mbean>
			</means>
			<graph_type>percentage</graph_type>
			<graph_multiplier>1000</graph_multiplier>
			<integer_value>false</integer_value>
		</graph>
	</graphs>
</jmx_template>